{
  "distributed-cache": {
    "mode": "SYNC",
    "remote-timeout": 17500,
    "state-transfer": {
      "timeout": 60000
    },
    "encoding": {
      "media-type": "text/plain"
    },
    "locking": {
      "concurrency-level": 1000,
      "acquire-timeout": 15000,
      "striping": false
    },
    "security": {
      "authorization": {
        "enabled": true,
        "roles": "monitor"
      }
    },
    "statistics": true
  }
}
